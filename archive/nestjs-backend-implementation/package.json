{
  "name": "ris-oms-backend",
  "version": "1.0.0",
  "description": "RIS OMS - Retail Intelligence System Order Management System Backend API",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\"",
    "start": "nest start",
    "start:dev": "cross-env NODE_ENV=dev && nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/src/main",
    "lint": "eslint \"src/**/*.ts\" --fix & prettier --write \"src/**/*.ts\"",
    "migration:create": "sequelize-cli migration:generate --env default --name",
    "migration:run": "sequelize-cli db:migrate --env default",
    "migration:revert": "sequelize-cli db:migrate:undo --env default",
    "migration:revert:all": "sequelize-cli db:migrate:undo:all --env default",
    "seed:create": "sequelize-cli db:seed:generate --env default --name",
    "seed:run": "sequelize-cli db:seed:all --env default",
    "seed:revert": "sequelize-cli db:seed:undo --env default",
    "seed:revert:name": "sequelize-cli db:seed:undo --env default --seed",
    "seed:revert:all": "sequelize-cli db:seed:undo:all --env default",
    "db:create": "sequelize-cli db:create --env default",
    "db:drop": "sequelize-cli db:drop --env default",
    "db:migrate": "sequelize-cli db:migrate --env default",
    "db:seed": "sequelize-cli db:seed:all --env default",
    "db:reset": "npm run db:drop && npm run db:create && npm run db:migrate && npm run db:seed",
    "docker:build": "docker build -t ris-oms-backend .",
    "docker:run": "docker run -p 3001:3001 ris-oms-backend",
    "docker:compose:up": "docker-compose up -d",
    "docker:compose:down": "docker-compose down",
    "health:check": "curl http://localhost:3001/health"
  },
  "dependencies": {
    "@nestjs/apollo": "^13.1.0",
    "@nestjs/common": "^11.0.1",
    "@nestjs/config": "^4.0.1",
    "@nestjs/core": "^11.0.1",
    "@nestjs/graphql": "^13.0.3",
    "@nestjs/mapped-types": "^2.1.0",
    "@nestjs/platform-express": "^11.0.1",
    "@nestjs/schedule": "^5.0.1",
    "@nestjs/sequelize": "^11.0.0",
    "@nestjs/swagger": "^11.0.6",
    "@nestjs/terminus": "^11.0.0",
    "@opentelemetry/api": "^1.9.0",
    "@opentelemetry/auto-instrumentations-node": "^0.57.0",
    "@opentelemetry/context-async-hooks": "^2.0.0",
    "@opentelemetry/core": "^2.0.0",
    "@opentelemetry/exporter-metrics-otlp-grpc": "^0.200.0",
    "@opentelemetry/exporter-prometheus": "^0.200.0",
    "@opentelemetry/exporter-trace-otlp-grpc": "^0.200.0",
    "@opentelemetry/exporter-trace-otlp-http": "^0.200.0",
    "@opentelemetry/instrumentation": "^0.202.0",
    "@opentelemetry/propagator-b3": "^2.0.0",
    "@opentelemetry/sdk-metrics": "^2.0.0",
    "@opentelemetry/sdk-node": "^0.200.0",
    "@opentelemetry/sdk-trace-base": "^2.0.0",
    "axios": "^1.8.4",
    "body-parser": "^1.20.3",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "dayjs": "^1.11.13",
    "graphql": "^16.10.0",
    "nestjs-otel": "^6.2.0",
    "@nestjs/swagger": "^11.0.6",
    "pg": "^8.13.3",
    "pg-hstore": "^2.3.4",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1",
    "sequelize": "^6.37.6",
    "sequelize-typescript": "^2.1.6",
    "short-uuid": "^5.2.0",
    "uuid": "^11.1.0",
    "winston": "^3.17.0",
    "winston-elasticsearch": "^0.19.0",
    "winston-logstash": "^1.2.1",
    "@nestjs/websockets": "^11.0.1",
    "@nestjs/platform-socket.io": "^11.0.1",
    "socket.io": "^4.8.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^11.0.0",
    "@types/express": "^5.0.0",
    "@types/node": "^22.10.7",
    "@typescript-eslint/parser": "^8.26.0",
    "cross-env": "^7.0.3",
    "eslint": "^9.21.0",
    "eslint-config-prettier": "^10.0.2",
    "prettier": "^3.5.3",
    "sequelize-cli": "^6.6.2",
    "ts-loader": "^9.5.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.7.3"
  }
}